{
  "name": "@powerfulyang/components",
  "version": "1.6.1",
  "description": "react-components",
  "author": "powerfulyang",
  "license": "MIT",
  "repository": "https://github.com/powerfulyang/components.git",
  "homepage": "https://github.com/powerfulyang/components#readme",
  "bugs": {
    "url": "https://github.com/powerfulyang/components/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.mjs",
  "style": "dist/es/index.css",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/es/index.css": {
      "style": "./dist/es/index.css"
    },
    "./dist/cjs/index.css": {
      "style": "./dist/cjs/index.css"
    }
  },
  "scripts": {
    "prettier": "prettier --write \"src/**/*.{tsx,ts,scss}\"",
    "lint:js": "eslint --fix --ext .ts,.tsx ./src",
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:source": "rollup -c",
    "build:types": "tsc -p tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "ts-check": "tsc --noEmit",
    "postprepare": "husky install",
    "test": "jest",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "dev": "vite",
    "build:site": "cross-env SITE=true vite build",
    "test:update:snapshot": "jest --updateSnapshot"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "peerDependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@powerfulyang/hooks": "workspace:*",
    "@powerfulyang/utils": "workspace:*",
    "classnames": "^2.3.2",
    "framer-motion": "^10.11.2",
    "immer": "^9.0.21",
    "jotai": "^2.0.4",
    "jotai-immer": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "optionalDependencies": {
    "d3": "^7.8.4"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@powerfulyang/hooks": "1.4.0",
    "@powerfulyang/lint": "3.1.3",
    "@powerfulyang/utils": "1.9.2",
    "@rollup/plugin-typescript": "11.1.0",
    "@rollup/plugin-url": "8.0.1",
    "@tanstack/react-query": "4.29.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/d3": "7.4.0",
    "@vitejs/plugin-react": "3.1.0",
    "classnames": "2.3.2",
    "framer-motion": "10.12.4",
    "immer": "9.0.21",
    "jotai": "2.0.4",
    "jotai-immer": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.10.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "typescript": "5.0.4",
    "vite": "4.2.1"
  }
}
