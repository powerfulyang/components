{
  "name": "@powerfulyang/components",
  "version": "1.7.4",
  "description": "react-components",
  "homepage": "https://github.com/powerfulyang/components#readme",
  "bugs": {
    "url": "https://github.com/powerfulyang/components/issues"
  },
  "repository": "https://github.com/powerfulyang/components.git",
  "license": "MIT",
  "author": "powerfulyang",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:source": "rollup -c",
    "build:types": "tsc -p tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "deploy:site": "cross-env SITE=true vite build",
    "dev": "vite",
    "lint:js": "eslint --fix --ext .ts,.tsx ./src",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write \"src/**/*.{tsx,ts,scss}\"",
    "test": "jest",
    "test:update:snapshot": "jest --updateSnapshot",
    "tsc-check": "tsc --noEmit"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@powerfulyang/hooks": "1.5.2",
    "@powerfulyang/utils": "1.11.2",
    "classnames": "2.3.2",
    "framer-motion": "10.12.16",
    "jotai": "2.2.0",
    "jotai-immer": "0.2.0",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@emotion/jest": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@jest/globals": "29.5.0",
    "@mui/material": "5.13.3",
    "@powerfulyang/lint": "3.3.0",
    "@rollup/plugin-typescript": "11.1.1",
    "@rollup/plugin-url": "8.0.1",
    "@tanstack/react-query": "4.29.12",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@types/testing-library__jest-dom": "5.14.6",
    "@vitejs/plugin-react": "4.0.0",
    "expect": "29.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "react-router-dom": "6.11.2",
    "typescript": "5.0.4",
    "vite": "4.3.9"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
